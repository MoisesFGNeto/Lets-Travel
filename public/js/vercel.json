{
  "version": 2,
  "builds": [
    {
      "src": "app.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/landmark",
      "dest": "/views/landmark.ejs"
    },
    {
      "src": "/admin",
      "dest": "/views/admin.ejs"
    },
    {
      "src": "/login",
      "dest": "/views/login.ejs"
    },
    {
      "src": "/requestPasswordReset",
      "dest": "/views/requestPasswordReset.ejs"
    },
    {
      "src": "/resetPassword",
      "dest": "/views/resetPassword.ejs"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
